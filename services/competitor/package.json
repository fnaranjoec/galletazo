{
  "name": "user",
  "version": "1.0.0",
  "description": "Galletazo User Service",
  "scripts": {
    "start": "npm run prod",
    "build": "npm run clean && npm run transpile && gulp && npm run viewscopy && npm run helperscopy",
    "server": "node ./dist-server/bin/www",
    "dev": "npm run build && cross-env DEBUG=static-service:* NODE_ENV=development npm run server",
    "prod": "npm run build && NODE_ENV=production npm run server",
    "transpile": "babel ./server --out-dir dist-server",
    "viewscopy": "babel ./server/views/**/*.pug --out-dir dist-server/views --copy-files",
    "helperscopy": "babel ./server/src/helpers/**/*.* --out-dir dist-server/src/helpers --copy-files",
    "clean": "rimraf dist-server && rimraf public",
    "watch-dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "events": {
      "restart": "kill-port 8101",
      "crash": "kill-port 8101"
    },
    "delay": "1500",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "keywords": [],
  "author": "Veritasoft S. A.",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@godaddy/terminus": "^4.4.1",
    "@google-cloud/storage": "^5.3.0",
    "autoprefixer": "^10.0.1",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.18.1",
    "circular-json": "^0.5.9",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cssnano": "^4.1.10",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "gulp": "^4.0.2",
    "gulp-babel-minify": "^0.5.1",
    "gulp-changed": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^9.0.0",
    "gulp-replace": "^1.0.0",
    "http-errors": "^1.8.0",
    "install": "^0.13.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.6",
    "npm": "^6.14.8",
    "path": "^0.12.7",
    "postcss": "^8.1.4",
    "pug": "^3.0.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "kill-port": "^1.6.1"
  }
}
